  <% if user_signed_in? %>

    <div class="row">
      <div class="col-md-1"></div>
      <div class="col-md-10">

        <!-- Show all FB Posts -->

        <% unless current_user.oauth_token == nil %>
        <h2>FB Posts</h2>
          <% @fblinks.each do |fblink| %>
            <%= render '/pages/fblink_post', :fblink => fblink %>
          <% end %>
        <% end %>

        <!-- Show All Wall Posts -->

        <h2> Posts </h2>
        <br/>
          <% @posts.each do |post| %>
            <%= render 'walls/post_row', :post => post %>
          <%end%>
      </div>
    </div>
  <%else%>

  <!-- Logged Out Home Page -->

  <%= render 'pages/landing_page' %>

 <%end%>
